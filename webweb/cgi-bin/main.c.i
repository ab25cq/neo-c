        return string(self);
    }

    auto result = new buffer.initialize();

    char* p = self;

    while(true) {
        char* p2 = strstr(p, str);

        if(p2 == null) {
            p2 = p;
            while(*p2) {
                p2++;
            }
            result.append(p, p2 - p);

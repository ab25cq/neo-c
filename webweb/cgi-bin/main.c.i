uniq int httpsd_socket(int port=443, _Bool reuse=0, void* parent, void (*block)(void* parent, SSL* it, _Bool* break_))
{
    int sock;
    SSL_CTX *ctx;

    OPENSSL_init_ssl(0x00200000L | 0x00000002L, ((void*)0));
    OPENSSL_init_ssl(0, ((void*)0));

    const SSL_METHOD *method;

    method = TLS_server_method();

    ctx = SSL_CTX_new(method);
    if (!ctx) {
        //ERR_print_errors_fp(stdout);
        die(s"Unable to create SSL context");
    }

    if (SSL_CTX_use_certificate_file(ctx, "cert.pem", 1) <= 0) {
        //ERR_print_errors_fp(stdout);
        die(s"SSL_CTX_use_certificate_file");
    }

    if (SSL_CTX_use_PrivateKey_file(ctx, "key.pem", 1) <= 0 ) {
        //ERR_print_errors_fp(stdout);
        die(s"SSL_CTX_use_PrivateKey_file");
    }

    sock = socket(2, 1, 0);
    if (sock < 0) {
        die(s"Unable to create socket");
    }

    struct sockaddr_in addr;
    addr.sin_family = 2;
    addr.sin_port = _OSSwapInt16(port);
    addr.sin_addr.s_addr = (u_int32_t)0x00000000;

    if (bind(sock, (struct sockaddr*)&addr, sizeof(addr)) < 0) {
        die(s"Unable to bind");
    }

    if (listen(sock, 1) < 0) {
        die(s"Unable to listen");
    }

    while (1) {
        struct sockaddr_in addr;
        uint len = sizeof(addr);
        int client = accept(sock, (struct sockaddr*)&addr, &len);

        if (client < 0) {
            die(s"Unable to accept");
        }

        SSL *ssl = SSL_new(ctx);
        SSL_set_fd(ssl, client);

        if (SSL_accept(ssl) <= 0) {
            ERR_print_errors_fp(__stdoutp);
        } else {
            _Bool break_ = 0;
            block(parent, ssl, &break_);

            if(break_) {
                break;
            }
        }

        SSL_shutdown(ssl);
        SSL_free(ssl);
        close(client);
    }

    close(sock);
    SSL_CTX_free(ctx);
    while(0) continue;

    return 0;
}

